{
  "name": "@trapts/route-decorator",
  "version": "1.0.6",
  "description": "Trap route decorator.",
  "scripts": {
    "start:example": "ts-node ./example/app.ts",
    "format": "prettier --write \"**/*.ts\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tsc": "tsc -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "ts",
    "trap",
    "koa"
  ],
  "repository": "https://github.com/TrapTS/route-decorator",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@trapts/route-decorator": "^1.0.6",
    "@trapts/router": "^1.0.0",
    "@types/koa": "^2.0.48",
    "@types/node": "^10.12.21",
    "dir_filenames": "^1.0.6",
    "husky": "^1.3.1",
    "koa": "^2.7.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  }
}
